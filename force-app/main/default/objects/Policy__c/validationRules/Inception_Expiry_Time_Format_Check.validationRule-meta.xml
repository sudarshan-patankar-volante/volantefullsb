<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Inception_Expiry_Time_Format_Check</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(Inception_Expiry_Time__c)),NOT(REGEX(Inception_Expiry_Time__c, &quot;^([0-9]|0[0-9]|1[0-9]|2[0-3]):[0-5][0-9]$&quot;)))</errorConditionFormula>
    <errorDisplayField>Inception_Expiry_Time__c</errorDisplayField>
    <errorMessage>Enter time in 24 hour format eg 14:40</errorMessage>
</ValidationRule>
