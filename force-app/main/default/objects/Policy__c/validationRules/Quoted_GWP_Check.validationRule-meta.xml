<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quoted_GWP_Check</fullName>
    <active>false</active>
    <errorConditionFormula>AND(Gross_Written_Premium_Rollup__c = 0, ISPICKVAL( Policy_Status__c , &quot;Quoted&quot;))</errorConditionFormula>
    <errorMessage>Policy premium before quoted stage has to be more than 0</errorMessage>
</ValidationRule>
