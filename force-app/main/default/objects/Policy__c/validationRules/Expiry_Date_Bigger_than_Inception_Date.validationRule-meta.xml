<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Expiry_Date_Bigger_than_Inception_Date</fullName>
    <active>true</active>
    <errorConditionFormula>VG_Inception_Date__c &gt; VG_Expiry_Date__c</errorConditionFormula>
    <errorDisplayField>VG_Inception_Date__c</errorDisplayField>
    <errorMessage>Expiry date must be greater than Inception date</errorMessage>
</ValidationRule>
