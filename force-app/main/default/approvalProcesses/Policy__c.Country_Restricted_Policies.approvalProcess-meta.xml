<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>allInternalUsers</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Edison_Motor</submitter>
        <type>group</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Gentium</submitter>
        <type>group</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Imara</submitter>
        <type>group</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>VG_Aesir</submitter>
        <type>group</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>VG_AFS</submitter>
        <type>group</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>VG_Bridge</submitter>
        <type>group</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>VG_Horizon</submitter>
        <type>group</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>VG_VFP</submitter>
        <type>group</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Policy_is_Approved_Email</name>
                <type>Alert</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>jennifer.allen@volanteglobal.com.vg</name>
                <type>user</type>
            </approver>
            <approver>
                <name>philip.goss@volanteglobal.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>vtc@volanteglobal.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Restricted Country Approval Pending</label>
        <name>Restricted_Country_Approval_Pending</name>
        <rejectionActions>
            <action>
                <name>Policy_is_Reject_Email</name>
                <type>Alert</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <emailTemplate>Allen_Approval/Allen_Email</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>AND ( NOT(VG_Is_Temp__c) ,OR( ISPICKVAL(Insured__r.ShippingCountryCode, &apos;CA&apos;), ISPICKVAL(Insured__r.ShippingCountryCode, &apos;AU&apos;),
ISPICKVAL(Insured__r.ShippingCountryCode, &apos;CH&apos;) ))</formula>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Country_Restricted_Approval_Email</name>
            <type>Alert</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>VG_Set_NTU_Or_Declined</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>VG_Set_Policy_Status</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>VG_Set_Sub_Status</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>true</finalRejectionRecordLock>
    <label>Country Restricted Policies</label>
    <processOrder>1</processOrder>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>false</showApprovalHistory>
</ApprovalProcess>
