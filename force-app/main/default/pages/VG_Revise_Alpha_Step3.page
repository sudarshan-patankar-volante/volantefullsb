                    <apex:outputPanel styleClass="requiredInput" layout="block" id="pwPanel2" >
                    <apex:outputPanel styleClass="requiredBlock"/>                          
                         <apex:inputField value="{!rr.VG_Aggregate__c}" html-placeholder="Enter Currency " />
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem rendered="{!(Product_Select != 'Aggregate Stop Loss' && Product_Select != 'Aggregate Excess of Loss')}">
