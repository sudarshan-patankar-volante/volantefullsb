                        rightLabel="Selected"
                        rightOption="{!SubCoverage_SelectedList}"
                        size="3"
                        width="150px"/>
                </apex:pageBlockSectionItem>
                
                <apex:pageBlockSectionItem rendered="{!Product_Select != 'Aggregate Stop Loss'}">
                    <apex:outputLabel >Limit</apex:outputLabel>
                     <apex:outputPanel styleClass="requiredInput" layout="block" id="pwPanel2" >
                     <apex:outputPanel styleClass="requiredBlock"/>                          
                        <apex:inputText value="{!limitperrisk}" html-placeholder="Enter Currency " />
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem rendered="{!Product_Select == 'Aggregate Stop Loss'}">
