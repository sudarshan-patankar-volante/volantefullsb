                            <apex:pageBlockSectionItem >
                                <apex:outputLabel >Epidemic Disease SI</apex:outputLabel>
                                <apex:inputtext value="{!currentCW.variable.SI}"/>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel >Deductible or waiting Period?</apex:outputLabel>
                                <apex:selectRadio value="{!currentCW.variable.dedOrWP}">
                                    <apex:actionSupport event="onchange" action="{!refreshView}" reRender="mainPageBlock"/>
                                    <apex:SelectOption itemValue="Deductible" itemLabel="Deductible"/>
                                    <apex:SelectOption itemValue="Waiting Preiod" itemLabel="Waiting Preiod"/>
                                </apex:selectRadio>
                            </apex:pageBlockSectionItem>
