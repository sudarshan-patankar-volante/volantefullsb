                                <apex:selectOptions value="{!currentCW.coveragesList}"></apex:selectOptions>
                            </apex:selectList>
                        </apex:pageBlockSectionItem>
                    </apex:pageBlockSection>
                    <apex:outputPanel rendered="{!addCoverageMode == 'Epidemic Disease'}">
                        <apex:pageBlockSection collapsible="false" columns="2">
