                            <apex:pageBlockSectionItem >
                                <apex:outputLabel >Freezer Deductible (PBA)</apex:outputLabel>
                                    <apex:inputtext value="{!currentCW.variable.PBA}"/>
                            </apex:pageBlockSectionItem>
                        </apex:pageBlockSection>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!addCoverageMode == 'Robbery'}">
                        <apex:pageBlockSection collapsible="false" columns="2">
                            <apex:repeat value="{!currentCW.factors}" var="price">
                                <apex:pageBlockSectionItem rendered="{!price.showOnUI}">
                                    <apex:outputLabel >{!price.factorName}</apex:outputLabel>
                                    <apex:selectList size="1" value="{!price.price }" >
                                        <apex:selectOptions value="{!price.factors}"></apex:selectOptions>
                                    </apex:selectList>
                                </apex:pageBlockSectionItem>
                            </apex:repeat>
                        </apex:pageBlockSection>
                        <apex:pageBlockSection >
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel >Robbery SI</apex:outputLabel>
                                <apex:inputtext value="{!currentCW.variable.SI}"/>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem />
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel >Robbery Deductible (Curr)</apex:outputLabel>
                                <apex:inputtext value="{!currentCW.variable.deductibleAmount}"/>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel >Robbery Deductible (PBA)</apex:outputLabel>
                                    <apex:inputtext value="{!currentCW.variable.PBA}"/>
                            </apex:pageBlockSectionItem>
                        </apex:pageBlockSection>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!addCoverageMode == 'Special Objects'}">
                        <apex:pageBlockSection collapsible="false" columns="2">
                            <apex:repeat value="{!currentCW.factors}" var="price">
                                <apex:pageBlockSectionItem rendered="{!price.showOnUI}">
                                    <apex:outputLabel >{!price.factorName}</apex:outputLabel>
                                    <apex:selectList size="1" value="{!price.price }" >
                                        <apex:selectOptions value="{!price.factors}"></apex:selectOptions>
                                    </apex:selectList>
                                </apex:pageBlockSectionItem>
                            </apex:repeat>
                        </apex:pageBlockSection>
                        <apex:pageblockTable value="{!soWrapperList}" var="SO">
                            <apex:column headerValue="Object">
                                <apex:selectList value="{!SO.SelectedObject}" size="1">
                                    <apex:selectOptions value="{!SO.obj}"></apex:selectOptions>
                                </apex:selectList>
                            </apex:column>
                            <apex:column headerValue="Sum Insured">
                                <apex:input value="{!SO.SI}"/>
                            </apex:column>
                            <apex:column headerValue="Location covered">
                                <apex:selectList value="{!SO.selectedLocation}" size="1">
                                    <apex:selectOptions value="{!SO.location}"></apex:selectOptions>
                                </apex:selectList>
                            </apex:column>
                            <apex:column headerValue="UW base rate" >
                                <apex:input value="{!SO.UWBaseRate}"/>
                            </apex:column>
                        </apex:pageblockTable>
                        <apex:pageBlockSection >
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel >Sp Obj Deductible (Curr)</apex:outputLabel>
                                <apex:inputtext value="{!currentCW.variable.deductibleAmount}"/>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel >Sp Obj Deductible (PBA)</apex:outputLabel>
                                    <apex:inputtext value="{!currentCW.variable.PBA}"/>
                            </apex:pageBlockSectionItem>
                        </apex:pageBlockSection>
                    </apex:outputPanel>
                    <apex:pageBlockSectionItem >
                        <apex:commandButton style="margin-left:350px;" value="Save" action="{!setCoveragePricings}"/>
                        <apex:commandButton value="Cancel" action="{!setCoveragePricings}"/>
                    </apex:pageBlockSectionItem>
                </apex:pageBlockSection>
            </apex:outputpanel>
            <apex:pageBlockSection title="Added Risk Ratings" columns="1">
                <apex:facet name="header"><apex:outputPanel ><apex:outputText >Added Risk Rating</apex:outputText><apex:commandButton style="margin-left:250px;" value="Add Risk Rating" action="{!addRiskRating}" reRender="mainPageBlock"/></apex:outputPanel></apex:facet>
                <apex:pagemessage rendered="{!CoverageIsEmpty}" severity="INFO">No Risk Ratings added</apex:pagemessage>
                <apex:pageblockTable rendered="{!NOT(CoverageIsEmpty)}" value="{!coverages}" var="CW">
                    <apex:column headerValue="Coverage" value="{!coverages[CW].selectedCoverage}"/>
                    <apex:column headerValue="Premium">
                        <apex:outputText value="{0, number, ###,##0}">
                            <apex:param value="{!coverages[CW].Premium}"/>
                        </apex:outputText>
                    </apex:column>
                </apex:pageblockTable>
                 <apex:outputPanel rendered="{!NOT(CoverageIsEmpty)}">
                    <div align="right" style="margin-right:80px;" >
                        <b><apex:outputLabel >Total Premium:&nbsp;</apex:outputLabel>
                        <apex:outputText value="{0, number, ###,##0}">
                            <apex:param value="{!totalpre}"/>
                        </apex:outputText></b>
                    </div>
                </apex:outputPanel>
            </apex:pageBlockSection>
        </apex:pageblock>
    </apex:form>
</apex:page>