                    <h1>Policy Schedule Details</h1><span class="my_text" align="right" style="margin-right:45px;"> <h3></h3></span>
                </div><br/><br/>
                <apex:pageBlocksection rendered="{!IsCancellation||IsExtension||IsReduction}" title="Mid Term Adjustment Details">
                    
                    <apex:pageBlockSectionItem rendered="{!IsCancellation || IsReduction}" >
                        <apex:outputLabel >Current Expiry Date</apex:outputLabel>
                        <apex:outputField value="{!ps.VG_Pre_MTA_Expiry_Date__c}"/>
                    </apex:pageBlockSectionItem>
                    <apex:pageBlockSectionItem rendered="{!IsCancellation}"  >
                        <apex:outputLabel >Cancellation Date</apex:outputLabel>
                        <apex:inputField value="{!ps.VG_MTA_Cancellation_Date__c}" style="Width:200px;" > 
